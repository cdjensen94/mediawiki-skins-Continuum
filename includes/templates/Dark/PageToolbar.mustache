<div class="continuum-page-toolbar">
	<div class="continuum-page-toolbar-container">
		<div id="left-navigation">
			<nav aria-label="{{msg-namespaces}}">
				{{#data-portlets}}{{#data-associated-pages}}{{>Tabs}}{{/data-associated-pages}}{{/data-portlets}}
				{{>Variants}}
			</nav>
		</div>
		<div id="right-navigation" class="continuum-collapsible">
			<nav aria-label="{{msg-views}}">
				{{#data-portlets}}{{#data-views}}{{>Tabs}}{{/data-views}}{{/data-portlets}}
			</nav>

			<nav class="continuum-page-tools-landmark" aria-label="{{msg-continuum-page-tools-nav-label}}">
				{{#data-page-tools-dropdown}}{{>Dropdown/Open}}{{/data-page-tools-dropdown}}
					{{#data-page-tools}}
					{{>PinnableContainer/Unpinned/Open}}
						{{! Use `#continuum-page-tools-unpinned-container .continuum-page-tools` to target styles at this element}}
						{{^is-pinned}}{{>PageTools}}{{/is-pinned}}
					{{>PinnableContainer/Close}}
					{{/data-page-tools}}
				{{#data-page-tools-dropdown}}{{>Dropdown/Close}}{{/data-page-tools-dropdown}}
			</nav>
		</div>
	</div>
</div>
